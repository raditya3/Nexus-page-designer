{
  "name": "page-designer-e2e",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/page-designer-e2e/src",
  "targets": {
    "e2e": {
      "executor": "@nx/cypress:cypress",
      "options": {
        "cypressConfig": "apps/page-designer-e2e/cypress.config.ts",
        "testingType": "e2e",
        "devServerTarget": "page-designer:serve:development"
      },
      "configurations": {
        "production": {
          "devServerTarget": "page-designer:serve:production"
        },
        "ci": {
          "devServerTarget": "page-designer:serve-static"
        }
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": [
        "{options.outputFile}"
      ]
    }
  },
  "tags": [],
  "implicitDependencies": [
    "page-designer"
  ]
}